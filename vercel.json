{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "cd frontend && npm install",
  "builds": [{ "src": "frontend/package.json", "use": "@vercel/next" }],
  "env": {
    "NEXT_PUBLIC_VERCEL_ENV": "production"
  },
  "github": {
    "silent": false,
    "autoAlias": true
  }
}
